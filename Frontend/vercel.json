{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VUE_APP_API_URL": "https://vueapp-navy-builder-production.up.railway.app",
    "VUE_APP_WS_URL": "wss://vueapp-navy-builder-production.up.railway.app/ws"
  }
} 